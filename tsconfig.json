{
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["ES2023"],
        "paths": {
            "@/*": ["./src/*"]
        },
        "module": "Node16",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}
